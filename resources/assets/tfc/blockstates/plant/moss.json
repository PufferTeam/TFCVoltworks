{
  "multipart": [
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_flat"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_side",
        "y": 180
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_side",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_hanging"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "tfc:block/plant/moss_side"
      }
    }
  ]
}
